) {\n    swapIFrame(item);\n  } else if (isImage) {\n    swapGif(item);\n  } else {\n    swapText(item);\n  }\n  \n  function swapGif(gif){\n    var gifId = $(gif).attr('id');\n    $(item).attr('src', pendoGif(gifId));\n  }\n\n  function swapText(text) {\n    var key = $(item).text(); \n    $(text).text(pendoI18n(key));\n  }\n\n  function swapIFrame(iFrame) {\n    var savedAttributes = {}\n    var iFrameId = iFrame.getAttributeNode(\"id\").value;\n    $.each(iFrame.attributes, function(idx, attr) {\n      if(this.specified) {\n        savedAttributes[this.name] = this.value;\n        if (attr.name ==='src') {\n          savedAttributes[this.name] = pendoVid(iFrameId);\n        }\n      }\n    });\n\n    $(item).replaceWith($('\u003ciframe\u003e', savedAttributes ));\n  }\n});\n\u003c/script\u003e","contentType":"text/html; charset=utf-8","rank":3593750,"advanceMethod":"button","thumbnailUrls":["https://storage.googleapis.com/pendo-static-5171025482350592/c7f6a36e-a26d-4313-536f-9306d642acb7"],"attributes":{"width":550,"autoHeight":false,"blockOutUI":{"additionalElements":[null]},"layoutDir":"auto","height":210,"css":"/* --- Selectors from Global CSS --- */\n\n._pendo-guide-container_ {\n    /* Guide border, shadow, background, etc. */\n    background-color: white;\n    padding: 30px;\n    /*color: #fff;*/\n    line-height: 140%;\n}\n\n._pendo-guide-container_ ._pendo-guide-content_ {\n    /* Content area: use for font attributes, padding, etc. */\n    text-align: left;\n    padding: 0\n}\n\n._pendo-guide-container_ ._pendo-close-guide_ {\n    /* Close guide icon */\n    display: inline;\n    font-size: 32px;\n    font-weight: 100;\n    top: 6px;\n    right: 15px;\n    color: #ccc;\n}\n\n._pendo-guide-container_ ._pendo-close-guide_:hover {\n    /* Close guide icon hover effects*/\n    color: #999;\n}\n\n/*Freemium specific CSS*/\n#_pendo-close-guide_ {\n    /*display: none;*/\n    padding-top: 16px;\n    padding-right: 40px;\n    height: 42px !important;\n}\n#_pendo