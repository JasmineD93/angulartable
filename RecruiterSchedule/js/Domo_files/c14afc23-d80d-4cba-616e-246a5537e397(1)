SCRIPT WRAPPER*/\n/* This function is used to pass over each div with text that should be internationalized. It must have the .pendo-l10n class and contain the correct key inside the div to work. */\n/* This is the old function\n$('.pendo-l10n').each(function(idx,\n item){ var key = $(item).text(); $(item).text(pendoI18n(key)); });\n */\n $('.pendo-l10n').each(function(idx,item) {\n  var isIFrame = $(item).is('iframe'); \n  var isImage = $(item).is('img');\n  \n  if(isIFrame) {\n    swapIFrame(item);\n  } else if (isImage) {\n    swapGif(item);\n  } else {\n    swapText(item);\n  }\n  \n  function swapGif(gif){\n    var gifId = $(gif).attr('id');\n    $(item).attr('src', pendoGif(gifId));\n  }\n\n  function swapText(text) {\n    var key = $(item).text(); \n    $(text).text(pendoI18n(key));\n  }\n\n  function swapIFrame(iFrame) {\n    var savedAttributes = {}\n    var iFrameId = iFrame.getAttributeNode(\"id\").value;\n    $.each(iFrame.attributes, function(idx, attr) {\n      if(this.specified) {\n        savedAttributes[this.name] = this.value;\n        if (attr.name ==='src') {\n          savedAttributes[this.name] = pendoVid(iFrameId);\n        }\n      }\n    });\n\n    $(item).replaceWith($('\u003ciframe\u003e', savedAttributes ));\n  }\n});\n\n/*END PENDO SCRIPT WRAPPER*/\u003c/script\u003e","contentType":"text/html; charset=utf-8","rank":5000000,"advanceMethod":"button","attributes":{"height":500,"width":900,"autoHeight":true,"css":"/* --- Selectors from Global CSS --- */\n\n._pendo-guide-container_ {\n    /* Guide border, shadow, background, etc. */\n    background-color: white;\n    padding: 30px;\n    /*color: #fff;*/\n    line-height: 140%;\n}\n\n._pendo-guide-container_ ._pendo-guide-content_ {\n    /* Content area: use for font attributes, padding, etc. */\n    text-align: left;\n    padding: 0\n}\n\n._pendo-guide-container_ ._pendo-close-guide_ {\n    /* Close guide icon */\n    display: inline;\n    font-size: 32px;\n    font-weight: 100;\n    top: 6px