   $(item).attr('src', pendoGif(gifId));\n  }\n\n  function swapText(text) {\n    var key = $(item).text(); \n    $(text).text(pendoI18n(key));\n  }\n\n  function swapIFrame(iFrame) {\n    var savedAttributes = {}\n    var iFrameId = iFrame.getAttributeNode(\"id\").value;\n    $.each(iFrame.attributes, function(idx, attr) {\n      if(this.specified) {\n        savedAttributes[this.name] = this.value;\n        if (attr.name ==='src') {\n          savedAttributes[this.name] = pendoVid(iFrameId);\n        }\n      }\n    });\n\n    $(item).replaceWith($('\u003ciframe\u003e', savedAttributes ));\n  }\n});\n\u003c/script\u003e","contentType":"text/html; charset=utf-8","rank":1718750,"advanceMethod":"element","thumbnailUrls":["https://storage.googleapis.com/pendo-static-5171025482350592/bea0e574-aab2-43e4-7c0b-160f43958c48"],"attributes":{"height":500,"width":500,"autoHeight":true,"css":"/* --- Selectors from Global CSS --- */\n\n._pendo-guide-container_ {\n    /* Guide border, shadow, background, etc. */\n    background-color: white;\n    padding: 30px;\n    /*color: #fff;*/\n    line-height: 140%;\n}\n\n._pendo-guide-container_ ._pendo-guide-content_ {\n    /* Content area: use for font attributes, padding, etc. */\n    text-align: left;\n    padding: 0\n}\n\n._pendo-guide-container_ ._pendo-close-guide_ {\n    /* Close guide icon */\n    display: inline;\n    font-size: 32px;\n    font-weight: 100;\n    top: 6px;\n    right: 15px;\n    color: #ccc;\n}\n\n._pendo-guide-container_ ._pendo-close-guide_:hover {\n    /* Close guide icon hover effects*/\n    color: #999;\n}\n\n/*Freemium specific CSS*/\n#_pendo-close-guide_ {\n    /*display: none;*/\n    padding-top: 16px;\n    padding-right: 40px;\n    height: 42px !important;\n}\n#_pendo-close-guide_:hover {\n    color: rgba(255,255,255,0.6) !important;\n}\n.freemium-lightbox-wrapper {\n    color: rgba(255,255,255,0.8);\n    font-size: 14px;\n    line-height: 20px !important;\n}\n.freemium-lightbox-wrapper .hidden {\n    d